{
	"name": "pinescript-helper",
	"displayName": "PineScript Helper",
	"description": "Language support for Pine Script V5 with Syntax Highlighting & Snippets",
	"icon": "images/pineicon.png",
	"publisher": "salbert11",
	"license": "MIT",
	"version": "3.2.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"author": {
		"name": "salbert11"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"pine",
		"pinescript",
		"Pine Script V5",
		"pine script",
		"pinecode",
		"pine code",
		"pineeditor",
		"pine editor",
		"tradingview",
		"trade",
		"trading"
	],
	"activationEvents": [
		"onLanguage:pine"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "pine",
				"path": "./snippets/built-in_language_operators.json"
			},
			{
				"language": "pine",
				"path": "./snippets/built-in_functions.json"
			},
			{
				"language": "pine",
				"path": "./snippets/built-in_variables_dictionnary.json"
			},
			{
				"language": "pine",
				"path": "./snippets/built-in_constants.json"
			},
			{
				"language": "pine",
				"path": "./snippets/built-in_scripts.json"
			},
			{
				"language": "pine",
				"path": "./snippets/built-in_types.json"
			},
			{
				"language": "pine",
				"path": "./snippets/comments.json"
			},
			{
				"language": "pine",
				"path": "./snippets/ect.json"
			},
			{
				"language": "pine",
				"path": "./snippets/f.json"
			},
			{
				"language": "pine",
				"path": "./snippets/loopscounters.json"
			},
			{
				"language": "pine",
				"path": "./snippets/principals.json"
			},
			{
				"language": "pine",
				"path": "./snippets/scripts_add.json"
			},
			{
				"language": "pine",
				"path": "./snippets/unicode.json"
			},
			{
				"language": "pine",
				"path": "./snippets/words.json"
			}
		],
		"languages": [
			{
				"id": "pine",
				"aliases": [
					"Pine Script",
					"PineScript",
					"Pine"
				],
				"extensions": [
					".ps",
					".pine",
					".pinescript",
					".pinecode",
					".tdps",
					".trading",
					".tradingview",
					".script"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "pine",
				"scopeName": "source.ps",
				"path": "./syntaxes/ps.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.4",
		"webpack": "^5.86.0",
		"webpack-cli": "^4.10.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/salbert11/pinescript.git"
	},
	"homepage": "https://github.com/salbert11/pinescript/blob/pinescript-helper/README.md",
	"dependencies": {
		"find-up": "^6.3.0",
		"fs": "^0.0.1-security",
		"import-local": "^3.1.0",
		"locate-path": "^7.2.0",
		"p-limit": "^4.0.0",
		"p-locate": "^6.0.0",
		"pkg-dir": "^7.0.0"
	}
}
